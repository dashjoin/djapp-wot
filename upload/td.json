{
    "@context": "https://www.w3.org/2022/wot/td/v1.1",
    "id": "urn:auth",
    "title": "Basic auth",
    "securityDefinitions": {
        "basic_sc": {
            "scheme": "basic",
            "in": "header"
        }
    },
    "security": "basic_sc",
    "properties": {
        "status": {
            "type": "string",
            "forms": [
                {
                    "href": "https://wot.run.dashjoin.com/rest/manage/version",
                    "contentType": "application/json",
                    "htv:methodName": "GET"
                }
            ]
        }
    },
    "credentials": "wot-cred",
    "role": "authenticated"
}